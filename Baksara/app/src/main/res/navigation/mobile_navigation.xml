<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_kelas">

    <fragment
        android:id="@+id/navigation_kelas"
        android:name="com.baksara.app.ui.kelas.KelasFragment"
        android:label="@string/home"
        tools:layout="@layout/fragment_kelas"
        />

    <fragment
        android:id="@+id/navigation_artikel"
        android:name="com.baksara.app.ui.ArtikelFragment"
        android:label="@string/berita"
        tools:layout="@layout/fragment_artikel"
        />

    <fragment
        android:id="@+id/navigation_pustaka"
        android:name="com.baksara.app.ui.PustakaFragment"
        android:label="@string/pustaka"
        tools:layout="@layout/fragment_pustaka"
        />

    <fragment
        android:id="@+id/navigation_profil"
        android:name="com.baksara.app.ui.ProfilFragment"
        android:label="@string/profile"
        tools:layout="@layout/fragment_profil"
        />

